<h3>Resend unlock instructions</h3>

<%= form_for(resource, as: resource_name, url: unlock_path(resource_name), html: { method: :post, :id => "form_login", class: "form" }) do |f| %>
  <%= devise_error_messages! %>
    <%= f.email_field :email, autofocus: true, placeholder: "Senha" %>
  </div>

  <div class="actions">
    <%= f.submit "Resend unlock instructions" %>
  </div>
<% end %>

<%= render "admin/users/shared/links" %>
